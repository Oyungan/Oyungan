myfirstarray[0] = "unuudur gradus bn"
myfirstarray[1] = "unuudur 1 gradus dulaan bn"
myfirstarray[2] = "unuudur 4 gradus dulaan bn"
myfirstarray[3] = "unuudur 9 gradus dulaan bn"
myfirstarray[4] = "unuudur 16 gradus dulaan bn"
myfirstarray[5] = "unuudur 25 gradus dulaan bn"

 for i = 1 to 5 step 1
    TextWindow.WriteLine(i+"="+myfirstarray[i])
 endfor
